 iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\n\n\n\n\n// Generated code.\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable;\n\nevent message1 = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  Brain.Screen.setFont(mono30);\n  Brain.Screen.print(\"Beyond: the Robotic Adventure...\");\n  Brain.Screen.setFillColor(white);\n  Brain.Screen.setPenWidth(8);\n  Brain.Screen.setFillColor(yellow);\n  Brain.Screen.setPenColor(orange);\n  Brain.Screen.drawCircle(200, 350, 200);\n  Brain.Screen.setPenColor(orange);\n  Brain.Screen.drawCircle(200, 350, 190);\n  Brain.Screen.setFillColor(white);\n  Brain.Screen.setPenColor(white);\n  Brain.Screen.drawCircle(70, 100, 50);\n  Brain.Screen.setPenWidth(5);\n  while (true) {\n    if (Brain.Screen.pressing()) {\n      message1.broadcastAndWait();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when I receive message1\" hat block\nvoid onevent_message1_0() {\n  Brain.Screen.setFillColor(black);\n  Brain.Screen.setPenColor(black);\n  Brain.Screen.drawCircle(300, 300, 500);\n}\n\n\nint main() {\n  // register event handlers\n  message1(onevent_message1_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}"}